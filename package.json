{
  "name": "automart",
  "version": "1.0.0",
  "description": "an online marketplace for automobile",
  "main": "index.js",
  "scripts": {
    "start:dev": "nodemon --exec babel-node ./SERVER/server",
    "clean": "rm -rf build && mkdir build",
    "build-babel": "babel -d ./build ./server -s",
    "build": "npm run clean && npm run build-babel",
    "start": "npm run build && node ./build/server.js",
    "test": "mocha './server/test/*.js'  --recursive || true"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/voke1/Atmrt.git"
  },
  "engines": {
    "node": "v10.15.3"
  },
  "keywords": [
    "online",
    "marketplace",
    "automobile"
  ],
  "author": "Voke Olomu",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/voke1/Atmrt/issues"
  },
  "homepage": "https://github.com/voke1/Atmrt#readme",
  "dependencies": {
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "dotenv": "^8.0.0",
    "cloudinary": "^1.14.0",
    "connect-multiparty": "^2.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.24.0",
    "morgan": "^1.9.1",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
